{
  "extends": ["../../.eslintrc.json"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "all",
        "endOfLine": "lf",
        "semi": false,
        "singleQuote": true,
        "printWidth": 80,
        "tabWidth": 2
      }
    ],
    "react/prop-types": "off",
    "react/react-in-jsx-scope": "off"
  }
}
